<div class="sign-up">
  <% form_for @user do |f|  %>
    <%= f.email_field :email %>
    <%= f.label :email %>

    <%= f.password_field :password %>
    <%= f.label :password %>

    <%= f.password_field :password_confirmation %>
    <%= f.label :password_confirmation %>

    <%= f.submit "Submit" %>
  <% end %>

</div>
